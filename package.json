{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "extension-sandbox",
  "title": "Extension Sandbox",
  "description": "A catch all personal extension to play around with functionality, ideas for extension commands",
  "icon": "command-icon.png",
  "author": "nbbaier",
  "license": "MIT",
  "commands": [
    {
      "name": "update",
      "title": "Update Command Metadata",
      "description": "Testing how updating metadata is supported",
      "mode": "no-view"
    },
    {
      "name": "dynamic-form",
      "title": "Dynamic Form",
      "description": "Seeing how dynamic forms work",
      "mode": "view"
    },
    {
      "name": "open",
      "title": "Open val.new",
      "description": "Lorem ipsum dolor sit amet.",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "key",
      "description": "some preference",
      "type": "textfield",
      "required": true,
      "title": "A preference",
      "placeholder": "put something here"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.9.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}